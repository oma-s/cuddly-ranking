{
  "expo": {
    "name": "cuddly-ranking",
    "slug": "cuddly-ranking",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo_transparent.png",
    "splash": {
      "image": "./assets/images/logo_transparent.png",
      "resizeMode": "contain",
      "backgroundColor": "#12071e",
      "dark": {
        "image": "./assets/images/logo_transparent.png",
        "backgroundColor": "#08040f"
      }
    },
    "scheme": "cuddlyranking",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.cuddlyranking.app",
      "buildNumber": "1",
      "supportsTablet": true,
      "icon": "./assets/images/ios-1024.png",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "NSCameraUsageDescription": "The app lets kids snap a photo of each cuddly toy.",
        "NSPhotoLibraryUsageDescription": "We show toy photos from your camera roll in their roster.",
        "NSPhotoLibraryAddUsageDescription": "Saving a toy photo may store it in your library."
      }
    },
    "android": {
      "package": "com.cuddlyranking.app",
      "versionCode": 1,
      "icon": "./assets/images/playstore.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo_transparent.png",
        "backgroundColor": "#12081f"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.CAMERA",
        "android.permission.READ_MEDIA_IMAGES"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon-180.png",
      "manifest": {
        "icons": [
          {
            "src": "./assets/images/favicon-48.png",
            "sizes": "48x48",
            "type": "image/png"
          },
          {
            "src": "./assets/images/favicon-96.png",
            "sizes": "96x96",
            "type": "image/png"
          },
          {
            "src": "./assets/images/favicon-180.png",
            "sizes": "180x180",
            "type": "image/png"
          },
          {
            "src": "./assets/images/favicon-192.png",
            "sizes": "192x192",
            "type": "image/png"
          },
          {
            "src": "./assets/images/favicon-256.png",
            "sizes": "256x256",
            "type": "image/png"
          },
          {
            "src": "./assets/images/favicon-384.png",
            "sizes": "384x384",
            "type": "image/png"
          },
          {
            "src": "./assets/images/favicon-512.png",
            "sizes": "512x512",
            "type": "image/png"
          }
        ]
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo_transparent.png",
          "imageWidth": 220,
          "resizeMode": "contain",
          "backgroundColor": "#12071e",
          "dark": {
            "backgroundColor": "#08040f"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "4b5d28a3-1002-4f3f-b8cd-e5d7f7e751e4"
      },
      "router": {}
    },
    "owner": "oma-s",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/4b5d28a3-1002-4f3f-b8cd-e5d7f7e751e4"
    }
  }
}
